---
// via @https://danidiaztech.com/create-astro-search-component/
export interface Props {
  dataType: string;
}
const { dataType } = Astro.props

import Search from './CourseSearch.jsx';
import { CMS_URL } from '@globals'
export const FEED_URL = `${CMS_URL}/feeds/complete.json`

const response = await fetch(FEED_URL)
const data = await response.json()
const courses = data['items']['courses'].filter((course) => course.live);

---

<Search client:load searchList={courses} dataType={dataType} />
